      {
        "type": "text",
        "id": "714",
        "folderId": "708",
        "text": "define piece-wise, but only for non-negative prices"
      },
      {
        "type": "expression",
        "id": "1",
        "folderId": "708",
        "color": "#c74440",
        "latex": "Q_{D}\\left(p\\right)=\\left\\{0\\le p\\le p_{max}:f\\left(p\\right),p>p_{max}:0\\right\\}",
        "hidden": true
      },
      {
        "type": "expression",
        "id": "736",
        "folderId": "708",
        "color": "#2d70b3",
        "latex": "P_{D}\\left(q\\right)=\\left\\{0\\le q\\le q_{max}:\\frac{A}{B}-\\frac{q}{B},q>q_{max}:0\\right\\}",
        "hidden": true
      },
      {
