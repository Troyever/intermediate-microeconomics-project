        "id": "957",
        "folderId": "940",
        "color": "#388c46",
        "latex": "Q_{mid}=\\frac{\\left(Q_{1}+Q_{2}\\right)}{2}"
      },
      {
        "type": "expression",
        "id": "958",
        "folderId": "940",
        "color": "#6042a6",
        "latex": "P_{mid}=\\frac{\\left(P_{1}+P_{2}\\right)}{2}"
      },
      {
        "type": "text",
        "id": "939",
        "folderId": "940",
        "text": "Delta P and Q are calculated in percentage form"
      },
      {
        "type": "expression",
        "id": "934",
        "folderId": "940",
        "color": "#000000",
        "latex": "D_{eltaP}=\\frac{\\operatorname{abs}\\left(P_{1}-P_{2}\\right)}{P_{mid}}"
      },
      {
        "type": "expression",
        "id": "935",
        "folderId": "940",
        "color": "#c74440",
        "latex": "D_{eltaQ}=\\frac{\\operatorname{abs}\\left(Q_{1}-Q_{2}\\right)}{Q_{mid}}"
      },
      {
        "type": "expression",
        "id": "936",
        "folderId": "940",
        "color": "#2d70b3",
        "latex": "E=\\frac{D_{eltaQ}}{D_{eltaP}}"
